{
  "name": "stenciljs-virtual-scroll",
  "version": "1.2.0",
  "description": "steniljs virtual scroll webcomponent",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest --no-cache",
    "test.watch": "jest --watch --no-cache"
  },
  "dependencies": {
    "@stencil/core": "0.9.11",
    "@stencil/utils": "0.0.4"
  },
  "devDependencies": {
    "@stencil/dev-server": "latest",
    "@stencil/sass": "0.0.5",
    "@stencil/utils": "latest",
    "@types/jest": "^21.1.1",
    "jest": "^21.2.1"
  },
  "keywords": [
    "virtual scroll",
    "scroll",
    "stenciljs",
    "webcomponent",
    "web components"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mgnstudio7/stenciljs-virtual-scroll.git"
  },
  "bugs": {
    "url": "https://github.com/mgnstudio7/stenciljs-virtual-scroll/issues"
  },
  "homepage": "https://github.com/mgnstudio7/stenciljs-virtual-scroll#readme",
  "author": "mgnstudio",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
